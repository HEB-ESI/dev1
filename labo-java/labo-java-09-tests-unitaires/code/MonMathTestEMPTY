package esi.dev1.td9;

import org.junit.Test;
import static org.junit.Assert.*;

/**
 *
 * @author ESI Prof
 */
public class MonMathTest {
   
    @Test
    public void testAbs() {
        System.out.println("abs");
        double x = 0.0;
        double expResult = 0.0;
        double result = MonMath.abs(x);
        assertEquals(expResult, result, 0.0);
    }
}
